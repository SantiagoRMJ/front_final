         return(\n                this.state.students.map(student => {\n                    return(\n                        <div className=\"body\" key={student._id}>\n                            <Link onClick={() => this.clickStudent(student)}>{this.capitalize(student.name)}</Link> \n                        </div>\n                    )\n                }))         \n        }else{\n            return(<div>CARGANDO LOS DATOS.</div>)\n        }   \n    }\n\n    \n\n    render() {\n        const token = JSON.parse(localStorage.getItem('token'))\n        return (\n            <>\n            <div className=\"nav-container\">\n                 <Link className=\"link\" to=\"/profesor/fichas\">Crear ficha</Link>\n                 \n                 <Link className=\"link\" to=\"/\" onClick={()=> this.logOut()}>Cerrar sesion</Link>\n            </div>\n            \n            <div className=\"show-students\">\n                <h1> Bienvenid@ {this.capitalize(token.name)}</h1>\n                <h2>Tus alumnos</h2>\n                <div className=\"students\" >{this.myStudents()}</div>\n            </div>\n        \n            </>\n\n        )\n    }\n}\n","/Users/santiagorodriguez-manzaneque/Desktop/front_final/front_final/src/components/SheetDetail/SheetDetail.js",["61"],"/Users/santiagorodriguez-manzaneque/Desktop/front_final/front_final/src/components/CreateSheet/CreateSheet.js",["62"],"/Users/santiagorodriguez-manzaneque/Desktop/front_final/front_final/src/components/Login/Login.js",["63"],"/Users/santiagorodriguez-manzaneque/Desktop/front_final/front_final/src/components/StudentDetail/StudentDetail.js",[],"/Users/santiagorodriguez-manzaneque/Desktop/front_final/front_final/src/components/LayoutStudent/LayoutStudent.js",["64"],"/Users/santiagorodriguez-manzaneque/Desktop/front_final/front_final/src/App.js",[],"/Users/santiagorodriguez-manzaneque/Desktop/front_final/front_final/src/components/SheetCorrection/SheetCorrection.js",[],"/Users/santiagorodriguez-manzanequ